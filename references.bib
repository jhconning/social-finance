@article{conning1999,
  title = {Outreach, {{Sustainability}} and {{Leverage}} in {{Monitored}} and {{Peer-Monitored}} Lending},
  shorttitle = {Outreach, {{Sustainability}} and {{Leverage}} in {{Monitored}} and {{Peer-Monitored}} Lending},
  author = {Conning, Jonathan H.},
  date = {1999},
  journaltitle = {Journal of Development Economics},
  volume = {60},
  number = {1},
  pages = {51--77},
  abstract = {This paper studies the contract design problem facing a microfinance lending organization (MFO) that aims to maximize the impact and outreach of its lending activities to a target population of poor borrowers while maintaining financial sustainability. A model of financial intermediation with moral hazard in the use of borrowed funds and in loan monitoring activities by delegates or peer-monitors predicts how a variety of different contracting strategies to lower  collateral requirements and increase loan sizes will be adapted to different target borrower groups.  Tradeoffs between  outreach, sustainability and financial leverage within these lending relationships are shaped by the monitoring and delegation costs that endogenously emerge within the chain of agency relationships established between borrowers and   loan-monitoring staff, staff and MFO owners, and between owners and outside investors and guarantors. All else equal, sustainable MFOs which target a poorer population of borrowers must charge higher interest rates, will employ more high-powered staff incentives, will have higher staff costs per dollar loaned, and will be able to leverage less of their equity. The models predictions are broadly supported by an analysis of evidence from 72 microfinance lending  programs from around the world and by evidence from the market for small business loans in the United States.},
  file = {H:\My Drive\B\zot_pdfs\Conning_1999_Outreach, Sustainability and Leverage in Monitored and Peer-Monitored lending.pdf}
}

