@article{graffzivin2005,
  title = {A {{Modigliani-Miller Theory}} of {{Altruistic Corporate Social Responsibility}}},
  shorttitle = {A {{Modigliani-Miller Theory}} of {{Altruistic Corporate Social Responsibility}}},
  author = {Graff Zivin, Joshua and Small, Arthur},
  date = {2005},
  journaltitle = {B E Journal of Economic Analysis and Policy: Topics in Economic Analysis and Policy},
  volume = {5},
  number = {1},
  pages = {1--19},
  issn = {1538-0653},
  abstract = {A new theory of altruistic corporate social responsibility is developed. Firms that advertise their social and environmental good works in effect solicit charitable contributions from customers, employees, investors and other stakeholders. They compete with not-for-profits in the market to supply public and altruistic goods. To analyze how corporate altruism affects firm valuations, a model is developed in which investors gain utility both from personal consumption and from making donations to worthy causes. A share in a "responsible" firm is a charity-investment bundle. When individuals view corporations and not-for-profits as equally competent suppliers of charity-related "warm glow," small changes in firms' social policies induce exactly offsetting changes in individuals' portfolio choices. There is no effect on firm valuations, and no change in the aggregate supply of good works. When a sizable fraction of investors prefer corporate philanthropy over direct charitable giving (e.g., to avoid taxation of corporate profits), firm valuations will be maximized by following social policies that involve strictly positive levels of corporate altruism.},
  langid = {english}
}

